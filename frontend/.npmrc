# npm設定ファイル
# プラットフォーム固有の依存関係を強制的にインストール
platform=linux
arch=x64

# オプショナル依存関係を無視しない
optional=false

# レジストリ設定
registry=https://registry.npmjs.org/

# キャッシュ設定
cache=.npm-cache

# ログレベル
loglevel=warn

# 依存関係の解決設定
legacy-peer-deps=true
strict-peer-dependencies=false

# プラットフォーム固有のバイナリを強制インストール
target_platform=linux
target_arch=x64
target_libc=glibc